<?xml version="1.0" encoding="utf-8"?>
<mapping>
  <entity target="Eccube\Entity\Cart">
    <field name="is_boomerang" type="boolean" column="is_boomerang">
      <options>
        <option name="default">false</option>
      </options>
    </field>
    <many-to-one field="bar" target-entity="Plugin\Boomerang\Entity\Bar">
      <join-columns>
        <join-column name="bar_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</mapping>
